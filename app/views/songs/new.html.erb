<h1>Add a Song</h1>

<%= form_for(@song) do |f| %>
<%= f.label :title %><br>
<%= f.text_field :title %><br>

<%= f.label :artist_name %><br>
<%= f.text_field :artist_name %><br>

<%= f.label :genre %><br>
<%= f.text_field :genre %><br>

<%= f.label :released %><br>
<%= f.check_box :released %> Yes<br>

<%= f.label :release_year %><br>
<%= f.number_field :release_year %><br>

<%= f.submit "Create" %>
<% end %>