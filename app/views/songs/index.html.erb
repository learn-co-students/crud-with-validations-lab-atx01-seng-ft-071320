<h1>Songs</h1>

<table>
    <thead>
      <tr>
        <th>Song</th>
        <th>Artist</th>
      </tr>
    </thead>
    <tbody>
        <% @songs.each do |song| %>
        <tr>
            <td><%= song.title %></td>
            <td><%= song.artist_name %></td>
        </tr>
        <% end %>
    </tbody>
  </table>

  <%= button_to "Add a Song", new_song_path, method: :get %>